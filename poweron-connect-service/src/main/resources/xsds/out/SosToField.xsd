  <xs:element name="SosToField">
    <xs:annotation>
      <xs:documentation>SOS sent to a Field Engineer.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element name="SosToFieldStc">
          <xs:complexType>
            <xs:all>
              <xs:element name="MsgHeader" type="ToMfsMsgHeader">
                <xs:annotation>
                  <xs:documentation>Standard message header to a Mobile Field System.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="MsgBody">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="ToUser" type="UserIdentificationType">
                      <xs:annotation>
                        <xs:documentation>The user to send the SOS to.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="Message" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>The SOS message to send to the field.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="Timestamp" type="xs:dateTime">
                      <xs:annotation>
                        <xs:documentation>The time the message was sent.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
            </xs:all>
          </xs:complexType>
        </xs:element>
      </xs:all>
    </xs:complexType>
  </xs:element>
</xs:schema>
