  <xs:complexType name="AlarmDetailsType">
    <xs:all>
      <xs:element name="AlarmId" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Alarm Id</xs:documentation>
        </xs:annotation>
      </xs:element>          
      <xs:element name="JobNumber" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Number of the job that the alarm belongs to.</xs:documentation>
        </xs:annotation>
      </xs:element>     
      <xs:element name="IncidentReference" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Reference of the incident that the alarm belongs to.</xs:documentation>
        </xs:annotation>
      </xs:element> 
      <xs:element name="Text" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Alarm text</xs:documentation>
        </xs:annotation>
      </xs:element> 
      <xs:element name="AlarmType" type="xs:integer" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Alarm type ID</xs:documentation>
        </xs:annotation>
      </xs:element> 
      <xs:element name="AlarmValue" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Alarm value</xs:documentation>
        </xs:annotation>
      </xs:element>    
    </xs:all>
  </xs:complexType>
</xs:schema>
